pipeline {
  agent any
  stages{    
    stage("compile"){
      steps {
        sh 'javac Testing.java'
      }  
    }
    
    stage("Run"){
      steps {
        sh 'java Testing'
      }  
    }

    stage("Docker"){
      steps {
        sh 'sudo docker build -t first-java-docker .'
        sh 'sudo docker run first-java-docker'
      }
    }
  }

}
